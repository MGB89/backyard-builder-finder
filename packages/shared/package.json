{
  "name": "@property-assessment/shared",
  "version": "1.0.0",
  "description": "Shared TypeScript types, utilities, and API client for Property Assessment application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
